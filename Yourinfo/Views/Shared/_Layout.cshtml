    <script src="~/Home/css/custom/js/jquery-data-to.js"></script>
    <script src="~/Home/css/vendor/jquery-circle-progress/circle.js"></script>
    <script src="~/Home/css/vendor/portfolio-filter-gallery/jquery.isotope.min.js"></script>
    <script src="~/Home/css/vendor/portfolio-filter-gallery/portfolio-filter-gallery.js"></script>
    <script src="~/Home/css/vendor/fancybox-master/jquery.fancybox.min.js"></script>
    <script src="~/Home/css/custom/js/custom.js"></script>
    <script>
        $(document).ready(function () {
            // Open the modal
            $("#openModalBtn").click(function () {
                $("#myModal").fadeIn();
            });

            // Close the modal when the close button is clicked
            $("#closeModalBtn").click(function () {
                $("#myModal").fadeOut();
            });

            // Close the modal when clicking outside the modal-content
            $(window).click(function (event) {
                if ($(event.target).is("#myModal")) {
                    $("#myModal").fadeOut();
                }
            });

        });

        $(document).ready(function () {
            $('#loginForm').on('submit', function (e) {
                e.preventDefault(); // Prevent the default form submission

                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("Login", "Home")', // Adjust the URL as needed
                    data: $(this).serialize(),
                    success: function (response) {
                        if (response.success) {
                            window.location.href = response.redirectUrl; // Redirect to another page
                        } else {
                            $('#loginMessage').html('<div class="alert alert-danger">' + response.message + '</div>');
                        }
                    },
                    error: function () {
                        $('#loginMessage').html('<div class="alert alert-danger">An error occurred. Please try again.</div>');
                    }
                });
            });
        });
    </script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>